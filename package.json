{
  "scripts": {
    "cy_run": "cypress run",
    "cy_run_recorded": "cypress run --record",
    "clean:reports": "rm -rf cypress/reports || true",
    "clean:Allure:reports": "rm -rf allure-results || true",
    "clean:reports:windows": "rd /s /q \"cypress\\reports\"\n",
    "clean:reports:allure": "rd /s /q \"allure-results\"\n",
    "clean:screenshots": "rm -rf cypress/screenshots || true",
    "clean:screenshots:windows": "rd /s /q \"cypress\\screenshots\"\n",
    "clean:videos": "rm -rf cypress/videos || true",
    "clean:videos:windows": "rd /s /q \"cypress\\videos\"\n",
    "clean:downloads": "rm -rf cypress/downloads || true",
    "clean:downloads:windows": "rd /s /q \"cypress\\downloads\"\n",
    "pretest": "npm run clean:reports && npm run clean:screenshots && npm run clean:videos && npm run clean:downloads && npm run clean:Allure:reports",
    "cy_run_key": "npx cypress run --record --key 3cc3ba98-fbc7-4c33-9ad1-90ebde8e1afd",
    "combine-reports": "npx mochawesome-merge \"cypress/reports/html/.jsons/*.json\" > mochawesome.json",
    "generate-report": "npx marge mochawesome.json -f report -o cypress/reports/html/.jsons/",
    "posttest": "npm run combine-reports && npm run generate-report",
    "posttest:Allure": "npx allure serve allure-results",
    "code_test": "npm run scripts || npm run posttest",
    "cy:open": "cypress open",
    "cy:run:smoke": "cypress run --headless --spec cypress/integration/productNames.spec.js --browser chrome",
    "cy:run:chrome": "cypress run --browser chrome --headed",
    "cy:run:chrome--spec": "cypress run --browser chrome --headed --spec",
    "cy:run:firefox": "cypress run --browser firefox --headed",
    "cy:run:electron": "cypress run --browser electron",
    "cy:run:chrome:headless": "cypress run --browser chrome --headless",
    "cy:run:chrome:headed": "cypress run --browser chrome --headed"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "allure-commandline": "^2.32.0",
    "cypress": "^14.3.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-react-selector": "^3.0.0",
    "cypress-repeat": "^2.3.8",
    "mocha": "^11.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "@cypress/github-action": "^6.7.8",
    "@cypress/grep": "^4.1.0",
    "@cypress/react": "^9.0.0",
    "@cypress/request": "^3.0.7",
    "@testing-library/cypress": "^10.0.2",
    "allure-cypress": "^3.2.1",
    "cy-verify-downloads": "^0.2.6",
    "cypress-fail-fast": "^7.1.1",
    "cypress-gmail-tester": "^1.0.0",
    "cypress-if": "^1.13.2",
    "cypress-iframe": "^1.0.1",
    "cypress-mailisk": "^2.0.1",
    "cypress-mailslurp": "^1.10.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-otp": "^1.0.3",
    "cypress-selectors": "^1.0.2",
    "cypress-slack-notify": "^1.14.10",
    "cypress-social-logins": "^1.14.2",
    "cypress-spellchecker": "^1.0.4",
    "cypress-try-or-skip": "^0.7.7",
    "cypress-wait-until": "^3.0.2",
    "typescript": "^5.7.3",
    "xpath_cypress": "^1.0.2"
  }
}
